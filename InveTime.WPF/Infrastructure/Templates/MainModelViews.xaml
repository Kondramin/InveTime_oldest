<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:system="clr-namespace:System;assembly=System.Runtime"
                    xmlns:vm="clr-namespace:InveTime.WPF.ViewModels.ViewsViewModels"
                    xmlns:views="clr-namespace:InveTime.WPF.Views.Views">

    <DataTemplate DataType="{x:Type system:Int32}">
        <TextBlock Text="{Binding StringFormat=int:{0}}"/>

    </DataTemplate>


    <DataTemplate DataType="{x:Type vm:StartViewModel}">
        <views:StartView DataContext="{Binding}"/>
    </DataTemplate>

</ResourceDictionary>